def labels = ['A', 'B'] // labels for Jenkins nodes
def builders = [:]
for (x in labels) {
    def label = x 
    builders[label] = {
      node(label) {
	echo "CHECKOUT TESTS X"
	echo "RUN TESTS X"
	echo "CHECKOUT TESTS Y"
	echo "RUN TESTS Y"
      	deleteDir()
	cleanWs()
      }
    }
}

parallel builders
